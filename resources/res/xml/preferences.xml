    <PreferenceCategory android:title="@string/pref_colorinspector_title" android:key="@string/pref_key_colorinspector_category">
        <ListPreference android:entries="@array/pref_samplesize_entries" android:title="@string/pref_samplesize_display" android:key="@string/pref_key_samplesize" android:defaultValue="@string/pref_samplesize_defaultvalue" android:dialogTitle="@string/pref_samplesize_dialogtitle" android:entryValues="@array/pref_samplesize_values"/>
        <CheckBoxPreference android:title="@string/pref_developercolors_display" android:key="@string/pref_key_developercolors" android:summary="@string/pref_developercolors_summary" android:defaultValue="@bool/pref_developercolors_defaultvalue"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/pref_colorfilter_title" android:key="@string/pref_key_colorfilter_category">
        <ListPreference android:entries="@array/pref_highlightcolor_entries" android:title="@string/pref_highlightcolor_display" android:key="@string/pref_key_highlightcolor" android:defaultValue="@string/pref_highlightcolor_defaultvalue" android:dialogTitle="@string/pref_highlightcolor_dialogtitle" android:entryValues="@array/pref_highlightcolor_values"/>
        <ListPreference android:entries="@array/pref_minsaturation_entries" android:title="@string/pref_minsaturation_display" android:key="@string/pref_key_minsaturation" android:defaultValue="@string/pref_minsaturation_defaultvalue" android:dialogTitle="@string/pref_minsaturation_dialogtitle" android:entryValues="@array/pref_minsaturation_values"/>
        <ListPreference android:entries="@array/pref_stripehue_entries" android:title="@string/pref_stripehue_display" android:key="@string/pref_key_stripehue" android:defaultValue="@string/pref_stripehue_defaultvalue" android:dialogTitle="@string/pref_stripehue_dialogtitle" android:entryValues="@array/pref_stripehue_values"/>
