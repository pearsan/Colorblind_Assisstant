<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:key="@string/pref_key_settings_screen">
    <PreferenceCategory android:title="@string/pref_general_title" android:key="@string/pref_key_general_category">
        <Preference android:title="@string/pref_about_display" android:key="@string/pref_key_about"/>
        <ListPreference android:entries="@array/pref_colorblindnesstype_entries" android:title="@string/pref_colorblindnesstype_display" android:key="@string/pref_key_colorblindnesstype" android:defaultValue="@string/pref_colorblindnesstype_defaultvalue" android:dialogTitle="@string/pref_colorblindnesstype_dialogtitle" android:entryValues="@array/pref_colorblindnesstype_values"/>
        <ListPreference android:entries="@array/pref_colornamepack_entries" android:title="@string/pref_colornamepack_display" android:key="@string/pref_key_colornamepack" android:defaultValue="@string/pref_colornamepack_defaultvalue" android:dialogTitle="@string/pref_colornamepack_dialogtitle" android:entryValues="@array/pref_colornamepack_values"/>
        <ListPreference android:entries="@array/pref_cameraquality_entries" android:title="@string/pref_cameraquality_display" android:key="@string/pref_key_cameraquality" android:defaultValue="@string/pref_cameraquality_defaultvalue" android:dialogTitle="@string/pref_cameraquality_dialogtitle" android:entryValues="@array/pref_cameraquality_values"/>
